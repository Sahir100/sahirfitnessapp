<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sahir.fitnessapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/web_hi_res_512"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainExercise">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".trainingplan" />
        <activity android:name=".Endurance" />
        <activity android:name=".Report" />
        <activity android:name=".Reminder" />
        <activity android:name=".Settings" />
        <activity android:name=".Quit" />
        <activity android:name=".Strength" />
        <activity android:name=".Jogging" />
        <activity android:name=".Climbing" />
        <activity android:name=".Walk" />
        <activity android:name=".Swim" />
        <activity android:name=".Rest" />
        <activity android:name=".Squat" />
        <activity android:name=".Pushup" />
        <activity android:name=".WeightLifting" />
        <activity android:name=".PullUp" />
        <activity android:name=".RestforStrength"></activity>

        <receiver android:name=".ReminderReceiver"
            android:enabled="true"
            android:exported="true"/>

    </application>

</manifest>